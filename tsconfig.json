{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "lib": ["es6"],
    "module": "commonjs",
    "outDir": "./dist/compile/",
    "strict": true,
    "baseUrl": "./"
  },
  "include": ["./src/**/*.ts"]
}
